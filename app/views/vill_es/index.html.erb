<h1>Listing villes</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>String</th>
      <th>Lattitude</th>
      <th>Float</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @villes.each do |ville| %>
      <tr>
        <td><%= ville.nom %></td>
        <td><%= ville.string %></td>
        <td><%= ville.lattitude %></td>
        <td><%= ville.float %></td>
        <td><%= ville.longitude %></td>
        <td><%= link_to 'Show', ville %></td>
        <td><%= link_to 'Edit', edit_ville_path(ville) %></td>
        <td><%= link_to 'Destroy', ville, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ville', new_ville_path %>
